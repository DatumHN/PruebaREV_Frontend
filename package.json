{
  "name": "@revfa/source",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.19 <25"
  },
  "scripts": {
    "//": "=== Seguridad en dependencias ===",
    "format": "npx nx format --check",
    "format:fix": "npx nx format --write",
    "lint": "nx run-many -t lint --parallel=3",
    "typecheck": "tsc -p tsconfig.base.json --noEmit",
    "build": "nx build source --configuration=production",
    "//_mfe_note": "Si se tienen varios MFEs, usar run-many:",
    "build:all": "nx run-many -t build --configuration=production --parallel=3",
    "test": "nx run-many -t test --parallel",
    "test:watch": "nx run-many -t test --watch",
    "test:affected": "nx affected -t test --base=origin/main --head=HEAD",
    "audit": "audit-ci --high --report-type summary",
    "kill": "powershell -Command \"Get-NetTCPConnection -State Listen | Where-Object { $_.LocalPort -like '420*' } | ForEach-Object { Stop-Process -Id $_.OwningProcess -Force }\"",
    "all": "npm run kill && nx run-many -t serve --projects=\"shell,registry,admin,auth,landing\" --parallel",
    "prepare": "husky",
    "dead-types": "ts-prune -p tsconfig.ts-prune.json",
    "dead-types:ci": "ts-prune -p tsconfig.ts-prune.json --error"
  },
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/common": "~20.1.0",
    "@angular/compiler": "~20.1.0",
    "@angular/core": "~20.1.0",
    "@angular/forms": "~20.1.0",
    "@angular/platform-browser": "~20.1.0",
    "@angular/platform-browser-dynamic": "~20.1.0",
    "@angular/router": "~20.1.0",
    "@ngx-formly/core": "^7.0.0",
    "@ngx-formly/primeng": "^7.0.0",
    "@primeuix/themes": "^1.2.3",
    "force": "^0.0.3",
    "lucide-angular": "^0.539.0",
    "pdfmake": "^0.2.20",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.1",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.1.0",
    "@angular-devkit/core": "~20.1.0",
    "@angular-devkit/schematics": "~20.1.0",
    "@angular/build": "~20.1.0",
    "@angular/cli": "~20.1.0",
    "@angular/compiler-cli": "~20.1.0",
    "@angular/language-service": "~20.1.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.8.0",
    "@module-federation/enhanced": "^0.17.0",
    "@nx/angular": "21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "^21.3.11",
    "@nx/js": "21.3.11",
    "@nx/module-federation": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/webpack": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@schematics/angular": "~20.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/pdfmake": "^0.2.11",
    "@typescript-eslint/parser": "^8.40.0",
    "@typescript-eslint/utils": "^8.29.0",
    "angular-eslint": "^20.0.0",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-preset-angular": "^15.0.0",
    "jiti": "2.4.2",
    "lint-staged": "^16.1.5",
    "nx": "21.3.11",
    "postcss": "^8.4.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^29.4.1",
    "ts-prune": "^0.10.3",
    "tslib": "^2.8.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0"
  }
}
