{
  "name": "auth",
  "sourceRoot": "apps/auth/src",
  "projectType": "application",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/auth",
        "index": "apps/auth/src/index.html",
        "main": "apps/auth/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/auth/tsconfig.app.json",
        "assets": ["apps/auth/src/favicon.ico", "apps/auth/src/assets"],
        "styles": ["apps/auth/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true,
          "buildOptimizer": false
        },
        "production": {
          "optimization": true,
          "sourceMap": false,
          "buildOptimizer": true,
          "extractLicenses": true,
          "namedChunks": false,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2MB",
              "maximumError": "5MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "200KB",
              "maximumError": "400KB"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "buildTarget": "auth:build",
        "port": 4202,
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "auth:build:development"
        },
        "production": {
          "buildTarget": "auth:build:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/admin/src/**/*.ts",
          "apps/admin/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/auth/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
